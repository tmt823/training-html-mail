<table
  border="0"
  width="800"
  cellpadding="0"
  cellspacing="0"
  bgcolor="#ffffff"
>
  <tbody>
    <tr>
      <td style="vertical-align: top; font-size: 0; text-align: center">
        <%- include('_separator', {height: 23}) %>
        <div>
          <img
            src="/assets/images/common/title_line.png"
            alt=""
            border="0"
            hspace="0"
            vspace="0"
            width="800"
            height="4"
          />
        </div>
        <%- include('_separator', {height: 20}) %>
        <div>
          <img
            src="<%= imagePath %>"
            alt="<%= imageAlt %>"
            width="<%= imageWidth %>"
            height="<%= imageHeight %>"
            border="0"
            hspace="0"
            vspace="0"
            style="vertical-align: top"
          />
        </div>
        <%- include('_separator', {height: 20}) %>
      </td>
    </tr>
  </tbody>
</table>
